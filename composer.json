{
    "name": "geoffreyrose/us-holidays",
    "description": "US Holidays Wrapper for the Carbon DateTime Library - Adds support for 42 US holidays + Your Custom Holidays",
    "keywords": ["Bank Holiday", "Holiday", "Holidays", "Carbon", "US Holiday", "US Holidays"],
    "license": "MIT",
    "require": {
        "nesbot/carbon": "^2.24",
        "php": "^8.0",
        "ext-calendar": "*"
    },
    "autoload": {
        "psr-4": {
            "USHolidays\\": "src/USHolidays/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "php-coveralls/php-coveralls": "^2.5"
    },
    "config": {
        "discard-changes": true
    }
}
